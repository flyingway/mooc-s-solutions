//problem 1


#include <stdio.h>

int main(){
    int a,b,c,d;
    c=0;d=0;
    while(1){
    scanf("%d",&a);
    if(a<0)
    break;
    b=a%2;
    if(b==1)
    c=c+1;
    else
    d=d+1;
}
printf("%d %d",c,d);
return 0;
}



// problem 2

#include <stdio.h>
#include <math.h>

int main(){
    int a,b,c,d,i,n,n1,sum;
    i=10,d=1,n=1,sum=0;
    scanf("%d",&a);
    while(a){
    b=a%10;
    a=a/i;
    n1=n%2;
    if(n1==1)
    d=1;
    else
    d=0;
    c=b%2;
    if(c==d)
    sum=pow(2,n-1)+sum;
    n=n+1;
}
printf("%d",sum);
}



